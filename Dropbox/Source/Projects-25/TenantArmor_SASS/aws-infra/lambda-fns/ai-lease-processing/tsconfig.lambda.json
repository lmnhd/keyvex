{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": ".",
    "rootDir": ".",
    "module": "commonjs",
    "target": "es2020",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": ["node"],
    "skipLibCheck": true
  },
  "include": [
    "index.ts"
  ],
  "exclude": [
    "node_modules",
    "../../node_modules"
  ]
} 