{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "NextJsS3ObjectActions",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject"
            ],
            "Resource": "arn:aws:s3:::tenantarmorstack-tenantarmoruploadsbuckete004fe66-mcf7amlrudwc/*"
        },
        {
            "Sid": "NextJsS3BucketActions",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": "arn:aws:s3:::tenantarmorstack-tenantarmoruploadsbuckete004fe66-mcf7amlrudwc"
        },
        {
            "Sid": "NextJsDynamoDBActions",
            "Effect": "Allow",
