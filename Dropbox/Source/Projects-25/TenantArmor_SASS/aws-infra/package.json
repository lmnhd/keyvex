{
  "name": "aws-infra",
  "version": "0.1.0",
  "bin": {
    "aws-infra": "bin/aws-infra-app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "prompts:list": "node scripts/manage-prompts.js list",
    "prompts:init": "node scripts/manage-prompts.js init",
    "prompts:get": "node scripts/manage-prompts.js get",
    "prompts:set": "node scripts/manage-prompts.js set",
    "prompts:update": "node scripts/manage-prompts.js update",
    "lambda:build": "cd lambda-fns/ai-lease-processing && npm run build",
    "deploy": "npm run lambda:build && npm run build && npx cdk deploy",
    "deploy:hotswap": "npm run lambda:build && npm run build && npx cdk deploy --hotswap"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.501.0",
    "@aws-sdk/client-secrets-manager": "^3.816.0",
    "@aws-sdk/lib-dynamodb": "^3.501.0",
    "aws-cdk-lib": "^2.147.0",
    "constructs": "^10.0.0",
    "openai": "^4.52.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "aws-cdk": "^2.147.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  }
}
